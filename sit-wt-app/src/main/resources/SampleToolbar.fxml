<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="sampleToolbar" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65"
  fx:controller="org.sitoolkit.wt.gui.pres.SampleToolbarController" style="-fx-padding: 0 0 0 0;" styleClass="tool-bar">
  <children>
    <Label fx:id="runSampleButton" onMouseClicked="#runSample" styleClass="image-btn,run-sample-btn">
      <tooltip>
        <Tooltip text="サンプルWebサイトを起動します。" />
      </tooltip>
    </Label>
    <Label fx:id="stopSampleButton" onMouseClicked="#stopSample" styleClass="image-btn,stop-sample-btn">
      <tooltip>
        <Tooltip text="サンプルWebサイトを停止します。" />
      </tooltip>
    </Label>
  </children>
</HBox>
